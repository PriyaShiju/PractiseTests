{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.js",
        "dist/jquery.min.js",
        "dist/jquery.min.map"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@5.3.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap-reboot.min.css",
        "dist/css/bootstrap-grid.min.css",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.min.css.map",
        "dist/css/bootstrap.rtl.min.css.map",
        "dist/css/bootstrap.rtl.min.css",
        "dist/css/bootstrap-utilities.rtl.min.css",
        "dist/css/bootstrap-utilities.rtl.min.css.map",
        "dist/css/bootstrap-utilities.min.css.map",
        "dist/css/bootstrap-utilities.min.css",
        "dist/css/bootstrap-grid.rtl.min.css",
        "dist/css/bootstrap-grid.rtl.min.css.map",
        "dist/css/bootstrap-reboot.min.css.map",
        "dist/css/bootstrap-grid.min.css.map",
        "dist/js/bootstrap.bundle.min.js",
        "dist/js/bootstrap.bundle.min.js.map",
        "dist/js/bootstrap.min.js.map",
        "dist/js/bootstrap.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@fortawesome/fontawesome-free@6.4.0",
      "destination": "wwwroot/lib/fortawesome/fontawesome-free/",
      "files": [
        "css/all.min.css",
        "css/brands.min.css",
        "css/fontawesome.min.css",
        "css/regular.min.css",
        "css/solid.min.css",
        "css/svg-with-js.min.css",
        "css/v4-font-face.min.css",
        "css/v4-shims.min.css",
        "css/v5-font-face.min.css",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff2",
        "webfonts/fa-v4compatibility.ttf",
        "webfonts/fa-v4compatibility.woff2"
      ]
    },
    {
      "provider": "unpkg",
      "library": "mmenu-js@9.3.0",
      "destination": "wwwroot/lib/mmenu-js/",
      "files": [
        "dist/mmenu.css",
        "dist/mmenu.js"
      ]
    }
  ]
}